<div> 
  <% @user.releases.each do |release| %>
  <a href="/releases/<%= release.id %>">
    <div class="album_card">
      <img src="<%= release.img_link %>"><br>
      <p class="title"><%= release.title %></p>
      <p class="artist"><%= release.artist %></p>
    </div>
  </a>
</div>
<% end %>
<br>
<div class="show_nav">
  <a class="button" href="/releases/new">Add to Collection</a>
  <a class="button" href="/tracks">All Tracks in Collection</a>
  <a class="button" href="/logout">Logout</a>
</div>

