<% @user.releases.each do |release| %>
  <img src="<%= release.img_link %>"><br>
  <%= release.title %><br>
  -<%= release.artist %><br>
  -<%= release.description %><br>
  <a href="/releases/<%= release.id %>">Details</a><br><br>
<% end %>
<br>
<div class="show_nav">
  <a class="button" href="/releases/new">Add a New Release</a>
  <a class="button" href="/tracks">All Tracks in Collection</a>
  <a class="button" href="/logout">Logout</a>
</div>

